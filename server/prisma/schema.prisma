// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Habit {
  id String @id @default(uuid()) // UUID Ã© um id unico universal -> Cria um unico id para cada id 
  title String
  created_at DateTime

  @@map("habits")
}
